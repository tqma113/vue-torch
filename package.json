{
  "name": "vue-torch",
  "version": "1.10.6",
  "description": "A lib to server-side render with vue.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "bin": {
    "torch": "./bin/torch.js"
  },
  "files": [
    "dist",
    "bin",
    "build.js",
    "build.d.ts",
    "dev.js",
    "dev.d.ts",
    "start.js",
    "start.d.ts",
    "AUTHORS"
  ],
  "scripts": {
    "start": "nodemon",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "release": "node scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postbuild": "node scripts/cp.js",
    "format": "prettier --write --parser typescript \"{src,__tests__,example}/**/*.ts?(x)\"",
    "test": "jest --silent",
    "dev:start": "node -r ts-node/register ./example/start.ts",
    "dev:dev": "node -r ts-node/register ./example/dev.ts",
    "dev:build": "node -r ts-node/register ./example/build.ts",
    "debug:start": "node --inspect-brk -r ts-node/register ./example/start.ts",
    "debug:dev": "node --inspect-brk -r ts-node/register ./example/dev.ts",
    "commit": "git-cz",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tqma113/vue-torch.git"
  },
  "keywords": [
    "vue",
    "ssr",
    "server-side-render"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tqma113/vue-torch/issues"
  },
  "homepage": "https://github.com/tqma113/vue-torch#readme",
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/fs-extra": "^9.0.2",
    "@types/gulp": "^4.0.7",
    "@types/gulp-plumber": "^0.0.32",
    "@types/gulp-uglify": "^3.0.6",
    "@types/helmet": "^4.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/mini-css-extract-plugin": "^1.2.0",
    "@types/morgan": "^1.9.1",
    "@types/serve-favicon": "^2.5.1",
    "@types/terser-webpack-plugin": "^5.0.0",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "@vue/server-renderer": "^3.0.2",
    "commitizen": "^4.2.2",
    "conventional-changelog": "^3.1.23",
    "conventional-changelog-cli": "^2.1.0",
    "css-loader": "^5.0.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "mini-css-extract-plugin": "^1.2.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "vue": "^3.0.2",
    "vuex": "^4.0.0-beta.4"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-do-expressions": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/webpack": "^4.41.23",
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/compiler-sfc": "^3.0.2",
    "babel-loader": "^8.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-spawn": "^7.0.3",
    "debug": "^4.2.0",
    "detect-port-alt": "^1.1.6",
    "express": "^4.17.1",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-plumber": "^1.2.1",
    "gulp-uglify": "^3.0.2",
    "helmet": "^4.1.1",
    "inquirer": "^7.3.3",
    "is-root": "^2.1.0",
    "morgan": "^1.10.0",
    "null-loader": "^4.0.1",
    "serve-favicon": "^2.5.0",
    "terser-webpack-plugin": "^5.0.2",
    "torch-history": "^1.0.1",
    "torch-router": "^1.0.0",
    "vue-loader": "16.0.0-beta.9",
    "webpack": "4",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
